    "resources":  [
                      {
                          "id":  "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/f8164078-0202-4ef8-91ca-e12795148110')]",
                          "name":  "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/f8164078-0202-4ef8-91ca-e12795148110')]",
                          "type":  "Microsoft.OperationalInsights/workspaces/providers/alertRules",
                          "kind":  "Scheduled",
                          "apiVersion":  "2023-12-01-preview",
                          "properties":  {
                                             "displayName":  "GWorkspace - Admin permissions granted - [WC-BASE-W]",
                                             "description":  "Triggers on admin permissions granted.",
                                             "severity":  "High",
                                             "enabled":  true,
                                             "query":  "GWorkspaceActivityReports \n| where EventMessage =~ 'grant_admin_privilege'\n| extend AccountCustomEntity = ActorEmail\n// WHITELIST START\n| extend RuleName = 'GWorkspace - Admin permissions granted'\n| join kind=leftouter (_GetWatchlist('WizardCyberAnalyticalRulesTuning')\n) on RuleName\n| project-away RuleName,RuleName1\n| extend Tuning01 = todynamic(Tuning01)\n| where not(  (TicketIDs contains \"3018012\" and isnotempty(Tuning01)) and AccountCustomEntity == Tuning01.[0])\n| project-away Tuning*,TicketIDs\n// WHITELIST END\n",
                                             "queryFrequency":  "PT1H",
                                             "queryPeriod":  "PT1H",
                                             "triggerOperator":  "GreaterThan",
                                             "triggerThreshold":  0,
                                             "suppressionDuration":  "PT1H",
                                             "suppressionEnabled":  false,

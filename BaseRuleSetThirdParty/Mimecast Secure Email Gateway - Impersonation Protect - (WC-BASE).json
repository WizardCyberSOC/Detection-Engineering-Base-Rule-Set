{
    "contentVersion":  "1.0.0.0",
    "$schema":  "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "resources":  [
                      {
                          "id":  "[concat(resourceId('Microsoft.OperationalInsights/workspaces/providers', parameters('workspace'), 'Microsoft.SecurityInsights'),'/alertRules/a9f25bba-731e-45b5-a4ee-8ebbadbce06b')]",
                          "name":  "[concat(parameters('workspace'),'/Microsoft.SecurityInsights/a9f25bba-731e-45b5-a4ee-8ebbadbce06b')]",
                          "type":  "Microsoft.OperationalInsights/workspaces/providers/alertRules",
                          "kind":  "Scheduled",
                          "apiVersion":  "2023-12-01-preview",
                          "properties":  {
                                             "displayName":  "Mimecast Secure Email Gateway - Impersonation Protect - [WC-BASE]",
                                             "description":  "Detects threats from impersonation mail under targeted threat protection",
                                             "severity":  "High",
                                             "enabled":  true,
                                             "query":  "MimecastSIEM_CL| where mimecastEventId_s == \"mail_ttp_impersonation\"",
                                             "queryFrequency":  "PT5M",
                                             "queryPeriod":  "PT5M",
                                             "triggerOperator":  "GreaterThan",
                                             "triggerThreshold":  0,
                                             "suppressionDuration":  "PT1H",
                                             "suppressionEnabled":  false,
                                             "startTimeUtc":  null,
                                             "tactics":  [
                                                             "DefenseEvasion",
                                                             "Reconnaissance"
                                                         ],
                                             "techniques":  [
                                                                "T1036",
                                                                "T1598"
                                                            ],
                                             "subTechniques":  [

                                                               ],
                                             "alertRuleTemplateName":  "7034abc9-6b66-4533-9bf3-056672fd9d9e",
                                             "incidentConfiguration":  {
                                                                           "createIncident":  true,
                                                                           "groupingConfiguration":  {
                                                                                                         "enabled":  false,
                                                                                                         "reopenClosedIncident":  false,
                                                                                                         "lookbackDuration":  "PT5M",
                                                                                                         "matchingMethod":  "AllEntities",
                                                                                                         "groupByEntities":  [

                                                                                                                             ],
                                                                                                         "groupByAlertDetails":  [

                                                                                                                                 ],
                                                                                                         "groupByCustomDetails":  [

                                                                                                                                  ]
                                                                                                     }
                                                                       },
                                             "eventGroupingSettings":  {
                                                                           "aggregationKind":  "SingleAlert"
                                                                       },
                                             "alertDetailsOverride":  null,
                                             "customDetails":  {
                                                                   "SimilarCustExtDomain":  "SimilarCustomExternalDomain_s",
                                                                   "ThreatDictionary":  "ThreatDictionary_s",
                                                                   "Hits":  "Hits_s",
                                                                   "SimilarIntDomain":  "SimilarInternalDomain_s",
                                                                   "ReplyMismatch":  "ReplyMismatch_s",
                                                                   "TaggedExternal":  "TaggedExternal_s",
                                                                   "SimilarMCExtDomain":  "SimilarMimecastExternalDomain_s",
                                                                   "Action":  "Action_s",
                                                                   "Route":  "Route_s",
                                                                   "Subject":  "Subject_s",
                                                                   "MsgId":  "MsgId_s",
                                                                   "InternalName":  "InternalName_s",
                                                                   "NewDomain":  "NewDomain_s",
                                                                   "TaggedMalicious":  "TaggedMalicious_s"
                                                               },
                                             "entityMappings":  [
                                                                    {
                                                                        "entityType":  "MailMessage",
                                                                        "fieldMappings":  [

                                                                                                  "columnName":  "Sender_s"
                                                                                              },
                                                                                              {
                                                                                                  "identifier":  "SenderIP",
                                                                                                  "columnName":  "IP_s"
                                                                                              },
                                                                                              {
                                                                                                  "identifier":  "Recipient",
                                                                                                  "columnName":  "Recipient_s"
                                                                                              }
                                                                                          ]
                                                                    }
                                                                ],
                                             "sentinelEntitiesMappings":  null,
                                             "templateVersion":  "1.0.0"
                                         }
                      }
                  ],
    "parameters":  {
                       "workspace":  {
                                         "type":  "String"
                                     }

{
    "contentVersion":  "1.0.0.0",
    "$schema":  "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "resources":  [
                      {
                          "id":  "[concat(resourceId(\u0027Microsoft.OperationalInsights/workspaces/providers\u0027, parameters(\u0027workspace\u0027), \u0027Microsoft.SecurityInsights\u0027),\u0027/alertRules/28810579-a6d6-4e13-aecb-396941cfa5dd\u0027)]",
                          "name":  "[concat(parameters(\u0027workspace\u0027),\u0027/Microsoft.SecurityInsights/28810579-a6d6-4e13-aecb-396941cfa5dd\u0027)]",
                          "type":  "Microsoft.OperationalInsights/workspaces/providers/alertRules",
                          "kind":  "Scheduled",
                          "apiVersion":  "2023-12-01-preview",
                          "properties":  {
                                             "displayName":  "Potentially Malicious URL Click Detected - [WC-BASE]",
                                             "description":  "Identifies when a user has clicked on a URL from an email that was classified as a phishing email.",
                                             "severity":  "Medium",
                                             "enabled":  true,
                                             "query":  "let LateFlag =  EmailPostDeliveryEvents\r\n    | where TimeGenerated \u003e ago(1h)\r\n    | where ActionType == \"Phish ZAP\"\r\n    | distinct NetworkMessageId;\r\nlet TooLate = UrlClickEvents\r\n    | where TimeGenerated \u003e ago(14d)\r\n    | join kind=inner LateFlag on NetworkMessageId\r\n    | join kind=inner EmailEvents on NetworkMessageId, $left.AccountUpn == $right.RecipientEmailAddress\r\n    | where not (OrgLevelAction == \"Allow\")\r\n    | where not(UserLevelPolicy contains \"Sender address list (Safe sender / Blocked sender)\")\r\n    | where not(SenderFromAddress contains \"knowbe4\")\r\n    | where not(SenderFromDomain contains \"knowbe4\")\r\n    | where not(InternetMessageId contains \"knowbe4\")\r\n    | distinct \r\n        TimeGenerated,\r\n        SenderFromAddress,\r\n        RecipientEmailAddress,\r\n        Subject,\r\n        ClickedURL = Url,\r\n        NetworkMessageId,\r\n        AuthenticationDetails,\r\n        SenderIPv4,\r\n        SenderIPv6,\r\n        SenderFromDomain,\r\n        UrlCount,\r\n        AttachmentCount,\r\n        ConfidenceLevel,\r\n        DetectionMethods,\r\n        DeliveryAction,\r\n        DeliveryLocation,\r\n        Type = \"Click Before the email was ZAPPED\";\r\nlet URLClick = UrlClickEvents\r\n    | where TimeGenerated \u003e ago(1h) // DEPENDS ON RULE FREQUENCY\r\n    | project AccountUpn, NetworkMessageId, Url;\r\nlet QuarantineRelease = EmailPostDeliveryEvents\r\n    | where Action == \"Quarantine release\" and ActionType == \"Manual Remediation\"\r\n    | project NetworkMessageId;\r\nEmailEvents\r\n| where TimeGenerated \u003e ago(14d)\r\n| join kind=leftouter EmailPostDeliveryEvents on NetworkMessageId\r\n| where ThreatTypes == \"Phish\" or ActionType == \"Phish ZAP\"\r\n| where not (OrgLevelAction == \"Allow\")\r\n| where not(UserLevelPolicy contains \"Sender address list (Safe sender / Blocked sender)\")\r\n| where not(SenderFromAddress contains \"knowbe4\")\r\n| where not(SenderFromDomain contains \"knowbe4\")\r\n| where not(InternetMessageId contains \"knowbe4\")\r\n| join kind=inner URLClick on NetworkMessageId, $left.RecipientEmailAddress == $right.AccountUpn\r\n| project\r\n    TimeGenerated,\r\n    SenderFromAddress,\r\n    RecipientEmailAddress,\r\n    Subject,\r\n    ClickedURL = Url,\r\n    NetworkMessageId,\r\n    AuthenticationDetails,\r\n    SenderIPv4,\r\n    SenderIPv6,\r\n    SenderFromDomain,\r\n    UrlCount,\r\n    AttachmentCount,\r\n    ConfidenceLevel,\r\n    DetectionMethods,\r\n    DeliveryAction,\r\n    DeliveryLocation\r\n| join kind=leftanti QuarantineRelease on NetworkMessageId\r\n| union TooLate\r\n| where SenderFromAddress !in (\"dse@eumail.docusign.net\", \"dse_na2@docusign.net\", \"dse_na3@docusign.net\")",
                                             "queryFrequency":  "PT1H",
                                             "queryPeriod":  "P14D",
                                             "triggerOperator":  "GreaterThan",
                                             "triggerThreshold":  0,
                                             "suppressionDuration":  "PT5H",
                                             "suppressionEnabled":  false,
                                             "startTimeUtc":  null,
                                             "tactics":  [
                                                             "Reconnaissance"
                                                         ],
                                             "techniques":  [
                                                                "T1598"
                                                            ],
                                             "subTechniques":  [

                                                               ],
                                             "alertRuleTemplateName":  null,
                                             "incidentConfiguration":  {
                                                                           "createIncident":  true,
                                                                           "groupingConfiguration":  {
                                                                                                         "enabled":  true,
                                                                                                         "reopenClosedIncident":  false,
                                                                                                         "lookbackDuration":  "P3D",
                                                                                                         "matchingMethod":  "AllEntities",
                                                                                                         "groupByEntities":  [

                                                                                                                             ],
                                                                                                         "groupByAlertDetails":  [

                                                                                                                                 ],
                                                                                                         "groupByCustomDetails":  [

                                                                                                                                  ]
                                                                                                     }
                                                                       },
                                             "eventGroupingSettings":  {
                                                                           "aggregationKind":  "SingleAlert"
                                                                       },
                                             "alertDetailsOverride":  null,
                                             "customDetails":  {

                                                               },
                                             "entityMappings":  [
                                                                    {
                                                                        "entityType":  "Account",
                                                                        "fieldMappings":  [
                                                                                              {
                                                                                                  "identifier":  "Name",
                                                                                                  "columnName":  "RecipientEmailAddress"
                                                                                              }
                                                                                          ]
                                                                    },
                                                                    {
                                                                        "entityType":  "URL",
                                                                        "fieldMappings":  [
                                                                                              {
                                                                                                  "identifier":  "Url",
                                                                                                  "columnName":  "ClickedURL"
                                                                                              }
                                                                                          ]
                                                                    },
                                                                    {
                                                                        "entityType":  "MailMessage",
                                                                        "fieldMappings":  [
                                                                                              {
                                                                                                  "identifier":  "NetworkMessageId",
                                                                                                  "columnName":  "NetworkMessageId"
                                                                                              }
                                                                                          ]
                                                                    }
                                                                ],
                                             "sentinelEntitiesMappings":  null,
                                             "templateVersion":  null
                                         }
                      }
                  ],
    "parameters":  {
                       "workspace":  {
                                         "type":  "String"
                                     }
                   }
}